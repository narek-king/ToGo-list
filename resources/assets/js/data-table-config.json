{
  "routePrefix": "places",
  "readSuffix": "data",
  "name": "Enso Vue Datatable Example",
  "icon": ["fab", "vuejs"],
  "crtNo": true,
  "buttons": [
    {
      "type": "global",
      "icon": "file-excel",
      "class": "is-outlined",
      "event": "excel",
      "label": "Excel"
    },
    {
      "type": "global",
      "icon": "plus",
      "class": "is-success",
      "event": "create",
      "label": "Create"
    },
    {
      "type": "row",
      "icon": "pencil-alt",
      "class": "is-warning",
      "event": "edit"
    },
    {
      "type": "row",
      "icon": "trash-alt",
      "class": "is-danger",
      "event": "destroy",
      "confirmation" : true,
      "message" : "This is your custom confirmation. Are you sure?"
    }
  ],
  "columns": [
    {
      "label": "Name",
      "name": "name",
      "data": "name",
      "meta": [
        "searchable",
        "sortable"
      ]
    }, {
      "label": "Coordinates",
      "name": "coordinates",
      "data": "coordinates",
      "meta": [
        "searchable",
        "sortable",
        "clickable"
      ]
    }
  ]
}